{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
          "jsx": true
        },
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": [
        "plugin:prettier/recommended",
        "prettier/@typescript-eslint"
      ],
    "globals": { 
        "React": "writable"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
    "browser": true,
    "node": true
  }
}
  